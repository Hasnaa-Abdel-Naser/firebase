import 'package:cloud_firestore/cloud_firestore.dart';
//main
// use  =>  await Firebase.initializeApp();
class fire{
// to add new document
FirebaseFirestore firestore = FirebaseFirestore.instance;
 addNewProduct(String name){
    firestore.collection('Names').add(
      {
        'name': name,
      }
    );
  }
}
